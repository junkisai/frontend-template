{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "gen": "conc -c auto pnpm:gen:*",
    "gen:css": "tcm src",
    "dev": "conc -c auto pnpm:dev:*",
    "dev:astro": "astro dev --port=3000",
    "dev:css": "tcm src --watch",
    "build": "astro build",
    "lint": "conc -c auto pnpm:lint:*",
    "lint:astro": "astro check",
    "lint:biome": "pnpm dlx @biomejs/biome lint .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check './**/*.{astro,md,yml,yaml,json}'",
    "fmt": "conc -c auto pnpm:fmt:*",
    "fmt:biome": "biome format --write --unsafe .",
    "fmt:prettier": "prettier -w './**/*.{astro,md,yml,yaml,json}'"
  },
  "dependencies": {
    "@astrojs/react": "3.6.2",
    "astro": "4.16.7",
    "destyle.css": "4.0.1",
    "next": "15.0.1",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/ts-plugin": "1.10.4",
    "@packages/biome-config": "workspace:*",
    "@packages/prettier-config": "workspace:*",
    "@packages/tsconfig": "workspace:*",
    "@types/node": "22.7.9",
    "@types/react": "18.3.12",
    "concurrently": "9.0.1",
    "prettier-plugin-astro": "0.14.1",
    "typed-css-modules": "0.9.1"
  }
}
